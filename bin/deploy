#!/bin/bash
pushd "$(dirname "$0")/.."
export CI_DEPLOY_DIR="$PWD"
popd

export DEPLOY_STATUS='started'
./rollbar-deploy
retVal=$?
if [ $retVal -eq 0 ]; then
  ./run_deploy
fi

retVal=$?
if [ $retVal -eq 0 ]; then
  DEPLOY_STATUS='succeeded'
else
  DEPLOY_STATUS='failed'
fi
./rollbar-deploy
exit retVal
